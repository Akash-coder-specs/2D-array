#include<stdio.h>
#include<conio.h>

int main()
{
	int arr[3][3];
	int i=0,j=0;
	printf("enter the numbers:");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
		 printf("\nthe elements of [%d],[%d]=:",i,j);
		 scanf("%d",&arr[i][j]);
	    }
	}
 
	printf("the matrix is:\n");

	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("%d\t",arr[i][j]);
		}
		printf("\n");
	}
	return 0;
}
